{
    "env": {
        "commonjs": true,
        "es2021": true
    },
    "extends":[ "airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-console": "off",
      "no-underscore-dangle": ["error", { "allow": ["_id", ""]}],
      "import/no-useless-path-segments": ["error", {
        "noUselessIndex": true
      }]
    }
}
